    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="declaring variables"/>
            <declare name="num, i, r, sum" type="Integer" array="False" size=""/>
            <comment text="r is the looping variable"/>
            <output expression="&quot;Enter the number&quot;" newline="True"/>
            <input variable="i"/>
            <output expression="&quot;Enter the number of times to be added&quot;" newline="True"/>
            <input variable="num"/>
            <assign variable="sum" expression="0"/>
            <for variable="r" start="1" end="num" direction="inc" step="1">
                <assign variable="sum" expression="i+r"/>
                <output expression="i&amp;&quot;+&quot;&amp;r&amp;&quot;=&quot;&amp;sum" newline="True"/>
                <assign variable="sum" expression="0"/>
            </for>
        </body>
    </function>
